<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="4000"
        motion:autoTransition="animateToEnd">
        <KeyFrameSet>
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid0"
                motion:framePosition="100"
                android:alpha="0"
                android:rotation="360" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid1"
                motion:framePosition="100"
                android:alpha="0"
                android:rotation="180" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid2"
                motion:framePosition="100"
                android:alpha="0"
                android:rotation="90" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid3"
                motion:framePosition="100"
                android:alpha="0"
                android:rotation="360" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid4"
                motion:framePosition="100"
                android:alpha="0"
                android:rotation="450" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid4"
                motion:framePosition="50"
                motion:keyPositionType="deltaRelative"
                motion:percentY="-0.446"
                motion:percentX="1.076" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid1"
                motion:framePosition="30"
                motion:keyPositionType="deltaRelative"
                motion:percentY="0.865"
                motion:percentX="0.303" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid1"
                motion:framePosition="60"
                motion:keyPositionType="deltaRelative"
                motion:percentY="1.293"
                motion:percentX="1.132" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid2"
                motion:framePosition="15"
                motion:keyPositionType="deltaRelative"
                motion:percentY="0.566"
                motion:percentX="0.348" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid2"
                motion:framePosition="30"
                motion:keyPositionType="deltaRelative"
                motion:percentY="2.238"
                motion:percentX="0.79" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid2"
                motion:framePosition="50"
                motion:keyPositionType="deltaRelative"
                motion:percentY="2.928"
                motion:percentX="0.997" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid2"
                motion:framePosition="65"
                motion:keyPositionType="deltaRelative"
                motion:percentY="1.856"
                motion:percentX="1.345" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid3"
                motion:framePosition="15"
                motion:keyPositionType="deltaRelative"
                motion:percentX="0.549"
                motion:percentY="0.058" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid3"
                motion:framePosition="30"
                motion:keyPositionType="deltaRelative"
                motion:percentX="1.215"
                motion:percentY="0.213" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid3"
                motion:framePosition="50"
                motion:keyPositionType="deltaRelative"
                motion:percentX="1.398"
                motion:percentY="0.569" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid3"
                motion:framePosition="65"
                motion:keyPositionType="deltaRelative"
                motion:percentX="0.811"
                motion:percentY="0.815" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid4"
                motion:framePosition="65"
                motion:keyPositionType="deltaRelative"
                motion:percentX="1.407"
                motion:percentY="0.162" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid4"
                motion:framePosition="30"
                motion:keyPositionType="deltaRelative"
                motion:percentX="0.683"
                motion:percentY="-0.198" />
            <KeyPosition
                motion:motionTarget="@+id/ivCovid4"
                motion:framePosition="15"
                motion:keyPositionType="deltaRelative"
                motion:percentX="0.336"
                motion:percentY="0.491" />


            <KeyAttribute
                motion:motionTarget="@+id/ivCovid0"
                motion:framePosition="85"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid1"
                motion:framePosition="85"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid2"
                motion:framePosition="85"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid3"
                motion:framePosition="85"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid4"
                motion:framePosition="85"
                android:alpha="1" />

            <KeyAttribute
                motion:motionTarget="@+id/ivCovid0"
                motion:framePosition="90"
                android:alpha="0.7" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid1"
                motion:framePosition="90"
                android:alpha="0.7" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid2"
                motion:framePosition="90"
                android:alpha="0.7" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid3"
                motion:framePosition="90"
                android:alpha="0.7" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid4"
                motion:framePosition="90"
                android:alpha="0.7" />


            <KeyAttribute
                motion:motionTarget="@+id/ivCovid0"
                motion:framePosition="95"
                android:alpha="0.4" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid1"
                motion:framePosition="95"
                android:alpha="0.4" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid2"
                motion:framePosition="95"
                android:alpha="0.4" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid3"
                motion:framePosition="95"
                android:alpha="0.4" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid4"
                motion:framePosition="95"
                android:alpha="0.7" />





            <KeyAttribute
                motion:motionTarget="@+id/ivCovid5"
                motion:framePosition="88"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid5"
                motion:framePosition="90"
                android:alpha="0.6" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid5"
                motion:framePosition="95"
                android:alpha="0.8" />
            <KeyAttribute
                motion:motionTarget="@id/ivCovid5"
                motion:framePosition="90"
                android:scaleX="1" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid5"
                motion:framePosition="90"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@id/ivCovid5"
                motion:framePosition="100"
                android:scaleX="5" />
            <KeyAttribute
                motion:motionTarget="@+id/ivCovid5"
                motion:framePosition="100"
                android:scaleY="5" />

        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="60dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="60dp"
            motion:layout_constraintStart_toEndOf="parent"
            android:id="@+id/ivCovid4"
            android:layout_marginTop="420dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="60dp"
            android:layout_width="60dp"
            android:id="@+id/ivCovid0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:alpha="0" />
        <Constraint
            android:layout_height="60dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="60dp"
            android:id="@+id/ivCovid4"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:layout_height="60dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="60dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/ivCovid1"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent" />
        <Constraint
            android:layout_height="60dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="60dp"
            android:id="@+id/ivCovid3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent" />
        <Constraint
            android:layout_height="60dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="60dp"
            android:id="@+id/ivCovid2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/ivCovid5"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp" />
    </ConstraintSet>
</MotionScene>